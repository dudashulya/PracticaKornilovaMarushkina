<Window x:Class="PracticaKornilovaMarushkina.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaKornilovaMarushkina"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="3*"></RowDefinition>
            <RowDefinition Height="12*">
            </RowDefinition>
        </Grid.RowDefinitions>
        <Image x:Name="MainLogo" Height="50"  Width="50"  Source="/Resources/pngwing.com.png" />
        <Button Grid.Row="0" Width="50" Height="30" HorizontalAlignment="Right" Margin="25" x:Name="BackBtn" Click="BackBtn_Click" >Назад</Button>
        <Frame  Grid.Row="1" x:Name="MainFrame" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
